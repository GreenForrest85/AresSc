{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "pysc2Train",
            "type": "python",
            "request": "launch",
            "module": "pysc2.bin.agent",
            "console": "integratedTerminal",
            "args": [
              "--map", "Simple64",
              "--agent", "deep_agent.DeepAgent",
              "--rgb_minimap_size", "64,64",
              "--max_agent_steps", "0"
            ],
            "stopOnEntry": false,
            "debugOptions": [
              "RedirectOutput"
            ]
          },
          {
            "name": "pysc2TrainNew",
            "type": "python",
            "request": "launch",
            "module": "pysc2.bin.agent",
            "console": "integratedTerminal",
            "args": [
                "--map", "Simple64",
                "--agent", "ares_agent.AresAgent",
                "--agent_race", "terran",
                "--feature_minimap_size", "64,64",
                "--feature_screen_size", "84,84",                
                "--rgb_minimap_size", "64,64",
                "--rgb_screen_size", "84,84",
                "--max_agent_steps", "0",
                "--action_space", "FEATURES",
                "--use_feature_units", "True"
            ],
            "stopOnEntry": false,
            "debugOptions": [
              "RedirectOutput"
            ]
          },          
          {
            "name": "pysc2",
            "type": "python",
            "request": "launch",
            "module": "pysc2.bin.agent",
            "console": "integratedTerminal",
            "args": [
              "--map", "Simple64",
              "--agent", "sparse_agent.SparseAgent",
              "--agent_race", "T"
            ],
            "stopOnEntry": false,
            "debugOptions": [
              "RedirectOutput"
            ]
          },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}",
            "port": 3000,
            "secret": "my_secret",
            "host": "localhost"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Terminal (external)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Django"
            ]
        },
        {
            "name": "Python: Flask (0.11.x or later)",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ]
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "module.name"
        },
        {
            "name": "Python: Pyramid",
            "type": "python",
            "request": "launch",
            "args": [
                "${workspaceFolder}/development.ini"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Pyramid"
            ]
        },
        {
            "name": "Python: Watson",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/console.py",
            "args": [
                "dev",
                "runserver",
                "--noreload=True"
            ]
        },
        {
            "name": "Python: All debug Options",
            "type": "python",
            "request": "launch",
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "module": "module.name",
            "env": {
                "VAR1": "1",
                "VAR2": "2"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "arg1",
                "arg2"
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        }
    ]
}